{
  "name": "arvyjaar/interval-manager",
  "description": "Library for operations on intervals",
  "keywords": [
    "interval",
    "sets",
    "subtract",
    "union"
  ],
  "minimum-stability": "stable",
  "license": "MIT",
  "authors": [
    {
      "name": "Laurynas Valenta",
      "email": "laurynas.valenta@nfq.lt"
    },
    {
      "name": "Arvydas Jankauskas",
      "email": "arvydas.jankauskas@nfq.lt"
    }
  ],
  "require": {
    "php": ">=8.1"
  },
  "require-dev": {
    "phpunit/phpunit": "^9.6|^10",
    "phpstan/phpstan": "^1.10",
    "friendsofphp/php-cs-fixer": "^3.16",
    "squizlabs/php_codesniffer": "^3.7",
    "slevomat/coding-standard": "~8.0"
  },
  "autoload": {
    "psr-4": {
      "Jaar\\IntervalManager\\": "src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Jaar\\Test\\IntervalManager\\": "test"
    }
  },
  "config": {
    "allow-plugins": {
      "dealerdirect/phpcodesniffer-composer-installer": true
    }
  }
}
